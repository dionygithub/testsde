{{ data['viewuser'] }}

<blockquote class="blockquote bq-info mb-4">
    <p class="bq-title">Mis <span class="sub-titulo" >Tests Realizados!!!</span></p>
</blockquote>

<div class="table-responsive">
    <!--Table-->

    {% if data['viewdata']|length > 0 %}
        <table class="table table-hover table-striped w-auto">

            <!--Table head-->
            <thead>
            <tr>
                <th>#</th>
                <th>Test</th>
                <th>Preguntas</th>
                <th>Correctas</th>
                <th>Puntos ganados</th>
                <th class="hidden-phone">NÃºmero de intento</th>
                <th class="hidden-phone">Fecha</th>
            </tr>
            </thead>
            <!--Table head-->

            <!--Table body-->
            <tbody>

            {% for index, test in data['viewdata'] %}
                <tr>
                    <th scope="row"><i class="fas fa-info mx-1"></i></th>
                    <td>{{ test.name }}</td>
                    <td>{{ test.preguntas }}</td>
                    <td>{{ test.pregcorrectas }}</td>
                    <td><span class="badge badge-success">{{ test.puntosganados }}</span></td>
                    <td class="hidden-phone"><span class="btn-outline-info mx-1 waves-effect">{{ test.numintento }}</span></td>
                    <td class="hidden-phone"> <span class="trigger teal lighten-4">{{ test.fecha }}</span></td>

                </tr>
            {% endfor %}


            </tbody>
            <!--Table body-->
        </table>
        <!--Table-->
    {% else %}
        <h3>No hay informaci&oacute;n que mostrar</h3>
    {% endif %}

</div>

